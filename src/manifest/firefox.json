{
    "manifest_version": 2,
    "permissions": [
        "notifications",
        "tabs",
        "*://*/*"
    ],
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": false
    },
    "browser_action": {
        "default_title": "Sample popup",
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icon16.png",
            "32": "icon32.png",
            "48": "icon48.png",
            "128": "icon128.png"
        }
    },
    "browser_specific_settings": {
        "gecko": {
            "id": "browser-extension-sample-ba32107@github.io"
        }
    }
}
